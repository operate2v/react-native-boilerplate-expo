{
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "~": "./app",
          "assets": "./app/assets",
          "components": "./app/components",
          "constants": "./app/constants",
          "screens": "./app/screens",
          "utils": "./app/utils",
          "routes": "./app/routes"
        }
      }
    ]
  ],
  "presets": ["react-native"]
}
